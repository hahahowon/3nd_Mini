use QT25;
-- 사용자 테이블 생성
CREATE TABLE users (
    id VARCHAR(20) PRIMARY KEY,
    isManager BOOLEAN NOT NULL,
    name VARCHAR(50) NOT NULL,
    password VARCHAR(50) NOT NULL
);

